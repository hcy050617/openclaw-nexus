{
  "name": "@houchenyang/nexus",
  "version": "1.0.1",
  "type": "module",
  "description": "OpenClaw channel plugin for connecting to Nexus Gateway",
  "license": "MIT",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/hcy050617/openclaw-nexus"
  },
  "files": [
    "dist",
    "openclaw.plugin.json"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "prepublishOnly": "pnpm build"
  },
  "openclaw": {
    "extensions": [
      "./dist/index.js"
    ],
    "channel": {
      "id": "nexus",
      "label": "Nexus",
      "selectionLabel": "Nexus Gateway",
      "blurb": "Connect to Nexus Gateway for AI bot messaging.",
      "order": 80
    }
  },
  "keywords": ["openclaw", "nexus", "gateway", "channel", "plugin"],
  "dependencies": {
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/ws": "^8.5.10",
    "typescript": "^5.7.0"
  },
  "peerDependencies": {
    "openclaw": ">=2026.1.24"
  }
}
